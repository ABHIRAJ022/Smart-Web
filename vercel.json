{
    "version": 2,
    "builds": [
        {
            "src": "health_system/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "250mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "health_system/wsgi.py"
        }
    ]
}